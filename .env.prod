# Production Environment Configuration

# App Configuration
APP_NAME=Postpartum Recovery
ENVIRONMENT=production

# API Configuration
API_BASE_URL=https://api.postpartumapp.com
API_TIMEOUT=30000

# Firebase Configuration
FIREBASE_API_KEY=your_prod_firebase_api_key
