# Staging Environment Configuration

# App Configuration
APP_NAME=Postpartum Recovery [STAGING]
ENVIRONMENT=staging

# API Configuration
API_BASE_URL=https://staging-api.postpartumapp.com
API_TIMEOUT=30000

# Firebase Configuration
FIREBASE_API_KEY=your_staging_firebase_api_key
FIREBASE_PROJECT_ID=postpartum-recovery-staging
FIREBASE_MESSAGING_SENDER_ID=your_staging_sender_id
FIREBASE_APP_ID=your_staging_app_id

# RevenueCat Configuration
REVENUECAT_API_KEY=your_staging_revenuecat_key

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_CRASH_REPORTING=true
ENABLE_FIREBASE=true

# Debug
DEBUG_MODE=true
SHOW_DEBUG_BANNER=true
